<div id="most-played" class="grid-x">
    <div class="time cell">
      <h3>Most Played Artists</h2>
      <ol class="no-bullet">
      <% @dj.most_played_artist.each do |artist, count| %>
        <div class="grid-x">
          <span class="artist cell small-2"><%= link_to artist, dj_artist_path(@dj, artist.parameterize) %></span>
          <span class="count cell small-1"><%= count %></span>
        </div>
      <% end %>
      </dl>
    </div>
</div>
