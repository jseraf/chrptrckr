<div id="most-played">
    <div class="time">
      <h2>Most Played Artist</h2>
      <dl>
      <% @dj.most_played_artist.each do |artist, count| %>
        <dt><%= link_to artist, dj_artist_path(@dj, artist.parameterize) %></dt>
        <dd><%= count %></dd>
      <% end %>
      </dl>
    </div>
</div>
